{
    "name": "w3security",
    "version": "1.0.0-monorepo",
    "description": "w3security library and cli utility",
    "files": [
        "*"
    ],
    "directories": {
        "lib": "src",
        "test": "test"
    },
    "bin": {
        "w3security": "bin/w3security"
    },
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "clean": "npx rimraf wrapper_dist tsconfig.tsbuildinfo",
        "build": "tsc && cp -R src/generated wrapper_dist/",
        "test": "npx jest test/*",
        "postinstall": "node wrapper_dist/bootstrap.js exec"
    },
    "keywords": [
        "security",
        "vulnerabilities",
        "advisories",
        "audit",
        "w3security",
        "scan",
        "docker",
        "container",
        "scanning"
    ],
    "author": "w3security.io",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/w3security/w3security.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "devDependencies": {
        "jest": "^29.4.1",
        "rimraf": "^4.1.2",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@sentry/node": "^7.36.0",
        "global-agent": "^3.0.0"
    }
}